<%= form_with model: @programmer, url: programmer_path(@programmer), method: "patch", local: true do |form| %>
    <%= form.hidden_field :id, value:@programmer.id %>
    <%= form.label :name %>
    <%= form.text_field :name %>


    <%= form.label :level %>
    <%= form.select(:level, options_for_select([['Rookie', 'Rookie'], ['Expert', 'Expert']]), :include_blank => true) %>

    <%= form.label :photo_url %>
    <%= form.file_field :photo_url %>
    <%= form.submit "Save" %>

<% end %>
