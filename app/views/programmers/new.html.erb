<%= form_with scope: :programmer, url: programmers_path, local: true do |form| %>

    <%= form.label :name %>
    <%= form.text_field :name %>


    <%= form.label :level %>
    <%= form.select(:level, options_for_select([['Rookie', 'Rookie'], ['Expert', 'Expert']]), :include_blank => true) %>

    <%= form.label :photo_url %>
    <%= form.file_field :photo_url %>
    <%= form.submit "Create" %>

<% end %>
